{
  "functions": {
    "app.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/templates/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
