{
  "functions": {
    "app.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/.*",
      "dest": "templates/index.html"
    }
  ],
  "headers": [
    {
      "source": "static/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
